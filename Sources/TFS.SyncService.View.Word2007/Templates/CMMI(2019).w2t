<?xml version="1.0" encoding="utf-8" ?>
<MappingConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="W2T.xsd" 
                      ShowName="CMMI (2019)" 
                      DefaultMapping="false" 
                      EnableRefresh="true"
                      IgnoreFormatting="true"
                      UseStackRank="false">
  <Mappings>
    <Mapping WorkItemType="Epic" MappingWorkItemType="Epic" RelatedTemplate="Epic(2015).xml" AssignRegularExpression="Epic" AssignCellRow="1" AssignCellCol="1" ImageFile="standard.png">
      <Fields>
        <!-- Possible values for FieldValueType: 'PlainText' or 'HTML'. These values correspond to 'FieldValueType' enum. -->
        <!-- Possible values for Direction: 'TfsToOther' or 'OtherToTfs'. These values correspond to 'Direction' enum. -->
        <!-- 'DefaultValue' node description: -->
        <!-- Node not defined - default value will be not configurable -->
        <!-- Node defined with any text - default value will be configurable and the defined text will be used in document if the default value  still not exists -->
        <Field Name="System.Title" FieldValueType="PlainText" Direction="OtherToTfs" MappingTableRow="1" MappingTableCol="2">
          <DefaultValue ShowName="Title">Epic Title</DefaultValue>
        </Field>
        <Field Name="System.Id" FieldValueType="PlainText" Direction="TfsToOther" MappingTableRow="1" MappingTableCol="3" />
        <Field Name="System.Rev" FieldValueType="PlainText" Direction="TfsToOther" MappingTableRow="1" MappingTableCol="4" />
        <Field Name="System.AssignedTo" FieldValueType="DropDownList" Direction="OtherToTfs" MappingTableRow="2" MappingTableCol="3" />
        <Field Name="System.State" FieldValueType="PlainText" Direction="TfsToOther" MappingTableRow="2" MappingTableCol="4" />
        <Field Name="System.Description" FieldValueType="HTML" Direction="OtherToTfs" MappingTableRow="3" MappingTableCol="1"
               ShapeOnlyWorkaroundMode="AddSpace">
          <DefaultValue ShowName="Description">Epic Description</DefaultValue>
        </Field>
      </Fields>
      <Converters />
    </Mapping>

