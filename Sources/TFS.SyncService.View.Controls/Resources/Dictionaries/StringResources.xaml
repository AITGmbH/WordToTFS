    <System:String x:Key="ReferenceNameColumnHeader">Referenced Field Name</System:String>
    <System:String x:Key="ValueTypeColumnHeader">Value Type</System:String>
    
    <!--Overwrite Content Dialog-->
    <System:String x:Key="OverwriteHtmlContentWindowHeader">Overwrite HTML Content</System:String>
    <System:String x:Key="OverwriteHtmlContentWindowTitle">Overwrite HTML Content</System:String>
    <System:String x:Key="OverwriteHtmlContentInformation1">You have updated a work item field that is configured as HTML text in your local template but not on the server. Publishing this content to Team Foundation Server will lead to a loss of any existing formatting.</System:String>
    <System:String x:Key="OverwriteHtmlContentInformation2">You can overwrite the content of the cell with plain text, or you can publish it as HTML.</System:String>
    <System:String x:Key="OverwriteHtmlContentInformation3">Do you want to replace your formatting with plain text? This will lead to a loss of all formatting.</System:String>
    <System:String x:Key="OverwriteHtmlContentYesButton">Yes, Replace</System:String>
    <System:String x:Key="OverwriteHtmlContentNoButton">No, Publish as HTML</System:String>


    <!--Wrong Mapped Fields Dialog-->
    <System:String x:Key="WronglyMappedFieldsWindowHeader">Wrong Field Value Type</System:String>
    <System:String x:Key="WronglyMappedFieldsWindowTitle">Wrong Field Value Type</System:String>
    <System:String x:Key="WronglyMappedFieldsWindowInformation">The Field Value for a mapping in the local template contains a definition that is different from the definition of the server. This might result in formatting problems while publishing work items, especially when the HTML type is used. Please ensure to use the same type that is used in your process template definition</System:String>
