    <!--GetWorkItemsPanelView.xaml-->
    <System:String x:Key="CloseSavedQueryDialog" xml:space="preserve">Close &quot;Saved Query&quot;-Dialog.</System:String>
    <System:String x:Key="ResultsOfComparingMessageFirstLine" xml:space="preserve">The selected WordToTFS template does not support all work item types.
For this reason, WordToTFS does not show the following work items:</System:String>
    <!-- Global used -->
    <System:String x:Key="OkButtonText">OK</System:String>
    <System:String x:Key="CancelButtonText">Cancel</System:String>
    
    <!-- PublishingStateControl -->
    <System:String x:Key="PublishStateControlTopic">Publishing Workitems</System:String>
    <System:String x:Key="SkippedFilderTextBlock">Include work items that were skipped because they were up to date</System:String>
    <System:String x:Key="SuccessedFilterTextBlock">Include work items that were successfully published</System:String>
